{
  "version": 2,
  "builds": [
    {
      "src": "myproject/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/api/(.*)",  
      "dest": "myproject/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "myproject/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "myproject.settings",
    "SECRET_KEY": "@your-secret-key",
    "DEBUG": "False",
    "PYTHON_VERSION": "3.9.12"
  }
}

